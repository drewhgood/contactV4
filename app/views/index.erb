

<div class="contain bs-docs-container">
  <row>
    <h1>Contact List V4</h1>
    
  </row>

  <form class="bs-example bs-example-form" data-example-id="input-group-with-button">
    <div class="row">
      <div class="col-lg-3"></div>
      <div class="col-lg-3">
        <button id="addContactForm" class="btn btn-lrg input-block-level">Add Contact</button>
      </div>
      <div class="col-lg-3">

        <div class="input-group">
          <span class="input-group-btn">
            <button class="btn btn-default" type="button">Go!</button>
          </span>
          <input type="text" class="form-control" placeholder="Search for...">
        </div>

      </div>
      <div class="col-lg-3"></div>
    </div><!-- /.row -->
  </form>


  <row>
    <div class="col-md-2"></div>
    <div class="col-md-8 bs-example" data-example-id="hoverable-table">
      <table class="table table-hover">
        <thead>
          <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>
          </tr>
        </thead>
        <tbody id="contacts_list">
        </tbody>
      </table>
    </div>
    <div class="col-md-2"></div>
  </row>
     
</div>
 
 

